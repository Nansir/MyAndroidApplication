<resources>



    <style name="AppTheme" parent="AppBaseTheme"/>


    <style name="AppTheme.NoActionBar" parent="AppBaseTheme.NoActionBar"/>

    <style name="AppTheme.AppBarOverlay" parent="AppBaseTheme.AppBarOverlay" />

    <style name="AppTheme.PopupOverlay" parent="AppBaseTheme.PopupOverlay" />




    <declare-styleable name="FABToolbarLayout">
        <attr name="showDuration" format="integer" />
        <attr name="hideDuration" format="integer" />
        <attr name="bottomMargin" format="dimension" />
        <attr name="rightMargin" format="dimension" />
        <attr name="fadeInPivotX" format="dimension" />
        <attr name="fadeInPivotY" format="dimension" />
        <attr name="fadeInFraction" format="float" />
    </declare-styleable>

</resources>
